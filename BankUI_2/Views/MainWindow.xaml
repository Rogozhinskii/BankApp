<Window x:Class="BankUI_2.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        xmlns:core="clr-namespace:BankLibrary.Common;assembly=BankLibrary"
        Title="{Binding Title}" Height="350" Width="525" >
    <Grid>
        <TabControl>
            <TabItem Header="Main Tab">
                <ContentControl prism:RegionManager.RegionName="{x:Static core:CommonTypesPrism.cBankClient}"/>
            </TabItem>
        </TabControl>
        <!--<ContentControl prism:RegionManager.RegionName="ContentRegion" />-->
    </Grid>
</Window>
